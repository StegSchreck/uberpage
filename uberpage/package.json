  "private": true,
  "homepage": "https://stegschreck.github.io",
  "dependencies": {
    "eslint": "9.39.2",
    "eslint-plugin-react": "7.37.5",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",

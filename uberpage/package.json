      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^28.0.0",
    "vite": "^5.3.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.12.0"

{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [
      "error",
      140
    ]
  },
  "overrides": [{
    "files": [ "serviceWorker.js" ],
    "rules": {
      "no-param-reassign": 0,
      "no-console": 0
    }
  }]
}
